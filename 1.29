(define (simpson f a b n)
  (define h (/ (- b a) n))
  (define (y k)
    (f (+ a (* k h))))
  (define (factor k)
    (cond ((or (= 0 k) (= k n))
           1)
          ((odd? k)
           4)
          (else
           2)))
  (define (term k)
    (* (factor k) (y k)))
  (define (next k)
    (+ k 1))
  (* (/ h 3.0)
     (sum term 0.0 next n)))
;0.24999999999999992
(simpson cube 0 1 100)
;0.2500000000000003
(simpson cube 0 1 1000)
