(define (make-account balance right-password)
  (define (withdraw amount)
    (if (>= balance amount)
        (begin (set! balance (- balance amount))
               balance)
        "Insufficient funds"))
  (define (deposit amount)
    (set! balance (+ balance amount))
    balance)
  (define (dispatch password m)
    (cond ((and (eq? m 'withdraw) (eq? password right-password)) withdraw)
          ((and (eq? m 'deposit) (eq? password right-password)) deposit)
          ((not (eq? password right-password)) (error "Incorrect password" password))
          (else (error "Unknown request: MAKE-ACCOUNT" m))))
  dispatch)

(define acc (make-account 100 'secret-password))
;60
((acc 'secret-password 'withdraw) 40)
;Incorrect password secret-other-password
((acc 'secret-other-password 'withdraw) 50)
